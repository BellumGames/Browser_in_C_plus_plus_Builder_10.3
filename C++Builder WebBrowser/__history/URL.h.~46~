//---------------------------------------------------------------------------
#ifndef URLH
#define URLH
#include <iostream>
using namespace std;
//clasa mea aici
 class URL
 {
	private:

		typedef struct LRU
		{
			AnsiString url;
			LRU *next;
		}* node;

		node head;
		node curr;
		node temp;

	public:

		URL();
		~URL();
		void AddNode(AnsiString addData);
		void DeleteNode();
		AnsiString Last();

 };

 class HTML : public URL
 {
	private:
    public:
 }
//---------------------------------------------------------------------------
#endif

